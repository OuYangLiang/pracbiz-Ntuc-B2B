<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
    <package name="readStatusReport" extends="pracbiz-default" namespace="/readStatusReport"> 
        <action name="init" class="com.pracbiz.b2bportal.core.action.ReadStatusReportAction" method="init">
            <result>/WEB-INF/views/core/report/readStatusReport.jsp</result>
        </action>
        <action name="printExcel" class="com.pracbiz.b2bportal.core.action.ReadStatusReportAction" method="exportExcel">
            <interceptor-ref name="b2bportalDefaultStackWithoutHeaderSetting"/>
            <result name="success" type="stream">
          		<param name="contentType">application/vnd.ms-excel</param>
          		<param name="inputName">rptResult</param>
          		<param name="bufferSize">1024</param>
          		<param name="contentDisposition">attachment;filename="${rptFileName}.xls"</param>
           </result>
            <result name="input">/WEB-INF/views/core/report/readStatusReport.jsp</result>
        </action>
        <action name="checkExcelData" class="com.pracbiz.b2bportal.core.action.ReadStatusReportAction" method="checkExcelData">
            <result type="json">
                <param name="root">data</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
    </package>
</struts>